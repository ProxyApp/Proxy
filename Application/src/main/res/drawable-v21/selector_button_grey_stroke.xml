<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/common_proxy_light_selected">
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/common_proxy_light_primary"/>
            <stroke android:width="2dp" android:color="@color/common_proxy_light_primary"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/common_proxy_dark_disabled"/>
            <stroke android:width="2dp" android:color="@color/common_text"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
</ripple>

